{
    "manifest_version": 3,
    "name": "Tab Garbage Collector",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "storage",
        "alarms",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icons/icon128.png"
    }
}